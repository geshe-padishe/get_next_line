for i in {0..100}
do
	printf "%d", "$i"
	rm -rf PAGE PAGE2
	< /dev/urandom tr -dc "\n\ts" | head -c10 > PAGE
	./a.out > PAGE2
	DIFF=$(diff PAGE PAGE2) > DIFF
	if [ "$DIFF" != "" ]
	then
		printf "\n_____________________________\n DIFF = "
		cat DIFF
		printf "\n_____________________________\n PAGE = "
		cat PAGE
		printf "\n_____________________________\n PAGE2 = "
		cat PAGE2
		printf "\n_____________________________\n"
	fi
	rm -rf PAGE PAGE2 DIFF
done
